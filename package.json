{
  "name": "posthtml-jsx",
  "version": "1.0.0",
  "description": "JSX Renderer for PostHTML",
  "main": "index.js",
  "engines": { "npm": ">=3","node": ">=4" },
  "scripts": {
    "clean": "rm -rf .nyc_output coverage jsxdoc-api",
    "lint": "standard",
    "pretest": "npm run clean && npm run lint",
    "test": "nyc ava 'test/index.js'",
    "docs:lib": "jsdoc2md lib/*.jsx > lib/lib.md",
    "docs:index": "jsdoc2md index.jsx > index.md",
    "docs": "npm run docs:index && npm run docs:lib",
    "release:major": "mversion major",
    "release:minor": "mversion minor",
    "release:patch": "mversion patch"
  },
  "dependencies": {
    "posthtml-render": "^1.0.6"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.14",
    "jsdoc-to-markdown": "^1.3.7",
    "mversion": "^1.10.1",
    "nyc": "^8.3.0",
    "posthtml": "^0.9.1",
    "standard": "^8.2.0"
  },
  "keywords": [
    "jsx",
    "html",
    "posthtml",
    "posthtmlrender"
  ],
  "author": {
    "name": "Michael Ciniawky",
    "email": "michael.ciniawsky@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posthtml/posthtml-jsx"
  },
  "bugs": {
    "url": "https://github.com/posthtml/posthtml-jsx/issues"
  },
  "homepage": "https://github.com/posthtml/posthtml-jsx#readme",
  "license": "MIT"
}
